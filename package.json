    "@types/cheerio": "^0.22.11",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/follow-redirects": "^1.8.0",
    "@types/morgan": "^1.7.35",
    "@types/request-promise-native": "^1.0.16",
    "@types/uuid": "^3.4.6",
    "ajax": "0.0.4",
    "axios": "^0.24.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "follow-redirects": "^1.11.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-libcurl": "^2.3.3",
    "request-promise-native": "^1.0.7",
